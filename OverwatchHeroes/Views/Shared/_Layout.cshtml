
  <!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="~/css/materialize.css"  media="screen,projection"/>

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body class="bodystyle">
        <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper">
                    <!--.navbar-material .nav-wrapper .brand-logo img {-->
                    <div class="row">
                        <ul id="nav-mobile" class="valign-wrapper">
                        <li class="col m5 s5 center"><a href="@Url.Action("Index","Home",null)" class="change-navtext-on-small-only">Home </a></li>
                        <li class="col m2 s2 center"><img class="logo-img valign-wrapper center-align" src="~/images/owlogo.png"></li>
                        <!-- Add alert to button with id "Repo" -->
                        <li class="col m5 s5 center"><a href="https://github.com/JP-x/OverwatchHeroes" rel="ext" class="change-navtext-on-small-only" id="githubnav">Repository </a> </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>

            @RenderBody()


        <footer class="page-footer">
            <div class="container">
                <div class="row">
                    <div class="center s12">
                        <a href="https://www.linkedin.com/in/jonathanpadilla4" id="linkedinicon">
                            <img class="responsive-img" src="~/images/linkedin_white.png" alt="LinkedIn" style="margin-right:20px">
                        </a>
                        <a href="https://github.com/JP-x">
                            <img class="responsive-img" src="~/images/github_white.png" alt="Github" id="githubicon">
                        </a>
                    </div>
                </div>
            </div>

            <div class="footer-copyright">
                <div class="container center">
                    Overwatch Fan-site. Not officially affiliated with Overwatch or Blizzard Entertainment®
                </div>
            </div>
        </footer>

            <!--Import jQuery before materialize.js-->
            <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
            <script type="text/javascript" src="~/js/materialize.js"></script>
            <script type="text/javascript" src="~/js/Init.js"></script>
    

    </body>



  </html>
        
